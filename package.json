    "test": "PORT=8001 REDIS_STORE_URI=redis://localhost:6379 REDIS_STORE_SECRET=secret TZ='America/Chicago' NODE_ENV=test ROOT_DIR=/home/mike/Development/backbone_postgres_seed/ mocha build/test -t 10000",
